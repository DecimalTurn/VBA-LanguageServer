{
	"VBA Pro: Public Property Get Let": {
		"prefix": "proplet",
		"description": "Property let and get with backing store",
		"body": [
			"Private m${1:PropertyName} As ${2:PropertyType}",
			"Public Property Let $1(var As $2)",
			"Attribute $1.VB_Description = \"${3:Dosctring}.\"",
			"'   $3.",
			"    m$1 = var",
			"End Property",
			"",
			"Public Property Get $1() As $2",
			"    $1 = m$1",
			"End Property",
			"",
			"$0"
		]
	},
	"VBA Pro: Public Property Get Set": {
		"prefix": "propset",
		"description": "Property set and get with backing store",
		"body": [
			"Private m${1:PropertyName} As ${2:PropertyType}",
			"Public Property Set $1(var As $2)",
			"Attribute $1.VB_Description = \"${3:Dosctring}.\"",
			"'   $3.",
			"    Set m$1 = var",
			"End Property",
			"",
			"Public Property Get $1() As $2",
			"    Set $1 = m$1",
			"End Property",
			"",
			"$0"
		]
	},
	"VBA Pro: Public Property Get": {
		"prefix": "propget",
		"description": "Property get only",
		"body": [
			"Public Property Get ${1:PropertyName}() As ${2:PropertyType}",
			"Attribute $1.VB_Description = \"${3:Dosctring}.\"",
			"'   $3.",
			"    $0",
			"End Property",
			""
		]
	},
	"VBA Pro: Subroutine": {
		"prefix": "sub",
		"description": "Subroutine",
		"body": [
			"${1:Public }Sub ${2:Identifier}($3)",
			"Attribute $2.VB_Description = \"${4:Dosctring}.\"",
			"'   $4.",
			"'",
			"'   Args:",
			"'       param1:",
			"'",
			"'   Raises:",
			"'",
			"",
			"    $0",
			"End Sub"
		]
	},
	"VBA Pro: Function": {
		"prefix": "func",
		"description": "Function",
		"body": [
			"${1:Public }Function ${2:Identifier}($3) As $4",
			"Attribute $2.VB_Description = \"${5:Dosctring}.\"",
			"'   $5.",
			"'",
			"'   Args:",
			"'       param1:",
			"'",
			"'   Returns:",
			"'",
			"'   Raises:",
			"'",
			"    Dim result As $4",
			"    $0",
			"",
			"    $2 = result",
			"End Function"
		]
	},
	"VBA Pro: Enum Exclusive": {
		"prefix": "enumexcl",
		"description": "Exclusive Enum",
		"body": [
			"Public Enum ${1:Identifier}",
			"    ${2:Enum1}",
			"    ${3:Enum2}",
			"    ${4:Enum3}",
			"End Enum"
		]
	},
	"VBA Pro: Enum Inclusive": {
		"prefix": "enumincl",
		"description": "Inclusive Enum",
		"body": [
			"Public Enum ${1:Identifier}",
			"    ${2:Enum1} = 2 ^ 1",
			"    ${3:Enum2} = 2 ^ 2",
			"    ${4:Enum3} = 2 ^ 3",
			"End Enum"
		]
	},
	"VBA Pro: ForLoop": {
		"prefix": "fori",
		"description": "For Loop",
		"body": [
			"${4:Dim $1 As Long}",
			"For ${1:i} = ${2:low} To ${3:high}",
			"    $0",
			"Next $1"
		]
	},
	"VBA Pro: ForEachLoop": {
		"prefix": "foreach",
		"description": "For Each Loop",
		"body": [
			"For Each ${1:object} In ${2:collection}",
			"    $0",
			"Next $1"
		]
	},
	"VBA Pro: If Block": {
		"prefix": "ifblock",
		"description": "If Block",
		"body": [
			"If ${1:condition} Then",
			"    $0",
			"End If"
		]
	},
	"VBA Pro: IfElse": {
		"prefix": "ifelse",
		"description": "If Else Block",
		"body": [
			"If ${1:condition} Then",
			"    $0",
			"Else",
			"End If"
		]
	},
	"VBA Pro: TypeCheckAssignment": {
		"prefix": "typecheck",
		"description": "Object type check assignment",
		"body": [
			"If IsObject(${1:obj?}) Then",
			"    Set ${2:Assignment}",
			"Else",
			"    $2",
			"End If$0"
		]
	},
	"VBA Pro: Case": {
		"prefix": "case",
		"description": "Select Case",
		"body": [
			"Select Case $1",
			"    Case Is = $2:",
			"        $0",
			"    Case Else:",
			"End Select"
		]
	},
	"VBA Pro: Event": {
		"prefix": "event",
		"description": "Event",
		"body": [
			"Public Event ${1:Identifier}($3)"
		]
	},
	"VBA Pro: Constructor": {
		"prefix": "ctor",
		"description": "Class_Initialize",
		"body": [
			"Private Sub Class_Initialize()",
			"    $0",
			"End Sub"
		]
	},
	"VBA Pro: Destructor": {
		"prefix": "dtor",
		"description": "Class_Terminate",
		"body": [
			"Private Sub Class_Terminate()",
			"    $0",
			"End Sub"
		]
	},
