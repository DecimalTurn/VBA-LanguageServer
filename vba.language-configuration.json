{
    "comments": {
        "lineComment": "'"
    },
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"]
    ],
    "autoClosingPairs": [
        { "open": "\"", "close": "\""},
        { "open": "(", "close": ")"},
        { "open": "[", "close": "]"}
    ],
    "autoCloseBefore": ": \n\r\t)}]\n",
    "surroundingPairs": [
        [ "(", ")" ],
        [ "[", "]" ],
        [ "\"", "\"" ]
    ],
    "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)",
}